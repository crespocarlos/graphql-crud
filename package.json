{
  "name": "lyrical",
  "version": "1.0.0",
  "description": "Starter point for a graphQL course",
  "private": true,
  "workspaces": [
    "frontend",
    "server"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "postinstall": "yarn bootstrap",
    "build:frontend": "yarn workspace @lyrical/frontend build",
    "build:server": "yarn workspace @lyrical/server build",
    "start:frontend": "yarn workspace @lyrical/frontend start",
    "start:server": "yarn workspace @lyrical/server start",
    "start:dev:server": "yarn workspace @lyrical/server start:dev",
    "start:dev": "npm run start:dev:server && npm run start:frontend",
    "test:server": "yarn workspace @lyrical/server test",
    "test:frontend": "yarn workspace @lyrical/frontend test",
    "test": "npm run test:frontend && npm run test:server"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@types/autoprefixer": "^9.7.2",
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^9.0.4",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/jest": "^26.0.15",
    "@types/mini-css-extract-plugin": "^1.2.1",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dev-utils": "^9.0.4",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-test-renderer": "^17.0.0",
    "@types/recompose": "^0.30.7",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.22.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
